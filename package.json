{
  "name": "fetch-ax",
  "version": "1.0.5",
  "description": "A modern HTTP client that extends the Fetch API, providing Axios-like syntax and full compatibility with Next.js App Router.",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Myongji-Graduate/fetch-ax.git"
  },
  "keywords": [
    "fetch",
    "axios",
    "http",
    "client",
    "fetch-ax",
    "Next.js"
  ],
  "author": "gahyuun",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.4.5"
  },
  "bugs": {
    "url": "https://github.com/Myongji-Graduate/fetch-ax/issues"
  },
  "homepage": "https://github.com/Myongji-Graduate/fetch-ax#readme",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/jest": "^29.5.12",
    "cross-fetch": "^4.0.0",
    "eslint": "^9.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.4"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --cache --write"
    ]
  }
}
